{
	"manifest_version": 2,

	"name": "Styled Background",
	"description": "Display home and new tab background with different background images",
	"version": "1.0.1",

	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	
	"browser_action": {
		"default_icon"	: "icon_medium.png",
		"default_popup"	: "popup.html"
	},

	"incognito"	: "not_allowed",

	"icons": {
		"200"	: "icon_full.png",
		"128"	: "icon_medium.png",
		"19"	: "icon_small.png"
	},

	"permissions": [
		"storage"
	],

	"background": {
	    "scripts"	: ["js/popup.js"],
	    "persistent": false
  	}
  	
}
